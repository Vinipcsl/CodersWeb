{
	"_version": "1.21.0",
	"sap.app": {
		"id": "sap.ui.demo.viniCelulares",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"applicationVersion": {
			"version": "1.0.0"
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},
	"sap.ui5": {
		"rootView": {
			"viewName": "sap.ui.demo.viniCelulares.view.App",
			"type": "XML",
			"id": "app"
		},
		"dependencies": {
			"minUI5Version": "1.98.0",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {}
			}
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "sap.ui.demo.viniCelulares.i18n.i18n",
					"supportedLocales": [
						""
					],
					"fallbackLocale": ""
				}
			}
		},		
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"type": "View",
				"viewType": "XML",
				"path": "sap.ui.demo.viniCelulares.view",
				"controlId": "app",
				"controlAggregation": "pages"
			},
			"routes": [
				{
					"pattern": "",
					"name": "listaDeCelulares",
					"target": "listaDeCelulares"
				},
				{
					"pattern": "Detalhe/{id}",
					"name": "detalhe",
					"target": "detalhe"
				}
			],
			"targets": {
				"listaDeCelulares": {
					"id": "celularesDaLista",
					"name": "ListaDeCelulares"
				},				
				"detalhe":{
					"id": "detalhe",
					"name": "Detalhe"
				}
			}
		}
	}
}